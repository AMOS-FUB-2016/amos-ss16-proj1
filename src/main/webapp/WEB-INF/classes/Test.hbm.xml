<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="de.fuberlin.chaostesting.hibernate.Test" table="TEST">
      <meta attribute="class-description">
         This class contains the user detail.
      </meta>
      <id name="id" type="int" column="id">
         <generator class="increment"/>
      </id>
      <property name="testVon" column="von" type="string"/>
      <property name="testNach" column="nach" type="string"/>
      <property name="testHinfahrt" column="hinfahrt" type="string"/>
	  <property name="testReisende" column="reisende" type="string"/>
	  <property name="testKlasse" column="klasse" type="string"/>
	  <property name="preis" column="preis" type="string"/>
	  
   </class>
</hibernate-mapping>